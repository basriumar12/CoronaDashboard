<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/parent_youtube"
    android:layout_height="match_parent"
    android:layout_margin="16dp"
    tools:context="com.basbas.lawanqfid.utama.ui.youtube.DetailYoutubeActivity">
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_youtube_detail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:listitem="@layout/item_berita" />

    <ProgressBar
        android:layout_centerInParent="true"
        android:id="@+id/progress_circular_youtube"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</RelativeLayout>
